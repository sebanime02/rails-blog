<%# Render the involucred states %>
<% @states_to_render_by_erb.each do |state| %>
    $("#<%= state %>_card_wrapper").html("<%= j render partial: 'v2/shortlists/shortlist_active_per_state_card', locals: {state: state}%>");
<% end %>

$("#change_available_dates_modal").modal('hide');
$("#notice_modal").find("#notice_message").html("<%= @notice_message %>");
$("#notice_modal").modal("show");
